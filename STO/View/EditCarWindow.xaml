<Window x:Class="STO.View.EditCarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STO.View"
        mc:Ignorable="d"
        Title="Редактирование машины" Height="400" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Width="200" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="New Make"/>
            <TextBox Text=""/>
            <TextBlock Text="New Model"/>
            <TextBox Text=""/>
            <TextBlock Text="New CarVin"/>
            <TextBox Text=""/>
            <TextBlock Text="New Year"/>
            <TextBox Text=""/>
            <TextBlock Text="New Color"/>
            <TextBox Text=""/>
        </StackPanel>
        <Button Grid.Row="1" Content="Редактировать машину"/>
    </Grid>
</Window>
